{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "disown-opener": "off",
    "css-prefix-order": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "backdrop-filter",
          "container-type",
          "container-type: inline-size",
          "text-wrap",
          "text-wrap: pretty",
          "text-wrap: balance"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ]
  }
}